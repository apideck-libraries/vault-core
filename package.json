{
  "version": "0.8.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && yarn build-tailwind",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build && yarn build-tailwind",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-tailwind": "NODE_ENV=production npx tailwindcss -o ./dist/tailwind.css --minify && yarn copy-css",
    "copy-css": "cp ./src/styles/base.css ./dist/styles.css && cp ./src/styles/custom.css ./dist/custom.css",
    "trigger-sync-workflow": "node ./scripts/triggerSyncWorkflow.js",
    "deploy": "np",
    "postdeploy": "yarn trigger-sync-workflow"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "@apideck/components": "^0.12.9",
    "formik": "^2.2.9",
    "fuse.js": "^6.5.3",
    "jwt-decode": "^3.1.2",
    "markdown-to-jsx": "^7.1.6",
    "react-select": "^5.2.2",
    "swr": "^1.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@apideck/react-vault",
  "author": "Jake Prins",
  "keywords": [
    "react",
    "vault",
    "apideck",
    "unify",
    "unified-apis",
    "integrations",
    "oauth"
  ],
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/react-vault.esm.js",
  "size-limit": [
    {
      "path": "dist/react-vault.cjs.production.min.js",
      "limit": "200 KB"
    },
    {
      "path": "dist/react-vault.esm.js",
      "limit": "200 KB"
    }
  ],
  "resolutions": {
    "jest-environment-jsdom": "^26.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@size-limit/preset-small-lib": "^7.0.5",
    "@storybook/addon-essentials": "next",
    "@storybook/addon-info": "next",
    "@storybook/addon-links": "next",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/react": "next",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^3.0.4",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "dotenv": "^16.0.3",
    "husky": "^7.0.4",
    "jest-location-mock": "^1.0.9",
    "node-fetch-commonjs": "^3.2.4",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^7.0.5",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^3.0.17",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "whatwg-fetch": "^3.6.2"
  }
}
